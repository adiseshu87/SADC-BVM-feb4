#include <stdio.h>
#include<string.h>

int main()
{
    char c,ch[10];
    int count=0,n,con=0,i;
  
    printf("enter string");
    gets(ch);
      n=strlen(ch);
      for(i=0;i<n;i++)
      {
          c=ch[i];
          switch(c)
          {
              case 'a' :
              case 'A' :
              case 'e' :
              case 'E' :
              case 'i' :
              case 'I' :
              case 'o' :
              case 'O' :
              case 'u' :
              case 'U' :count++;
              break;
              default:printf("%c",c);
              con++;
              
          }
      }
    printf("\n total no of vowels %d",count);
    printf("\n total no of consonants %d",con);
    

    return 0;
}
